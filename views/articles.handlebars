{{>nav}}
<section>
	<h1>All Articles</h1>
	<div id="all-articles" class="">
		<div id="results-wrapper">
				<table id="results-table">
					<tbody>
					{{#each article}}
						<tr>
							<td class="article-img" ><a href="/articles/{{this._id}}"><img src="{{this.image}}" width='160px' height='100px'></a></td>
							<td class="article-info">
								<span class="article-tag">{{this.label}}</span><br>
								<h2><a class="article-title" href="/articles/{{this._id}}">{{this.title}}</a></h2>
							</td>
						</tr>
					{{/each}}
					</tbody>
				</table>
		</div>
	</div>
</section>